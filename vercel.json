{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "http://localhost:8000/api/$1"
      },
      {
        "src": "/uploads/(.*)",
        "dest": "http://localhost:8000/uploads/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  